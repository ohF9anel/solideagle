<div id="group-content">

<?php 
	function render_tree($roots)
	{
		foreach($roots as $group)
		{
			echo "<ul>\n<li>";
			echo $group->getName() . "(" . $group->getId() . ")\n";
			render_tree($group->getChildGroups());
			echo "</li></ul>\n";
		}
	}
	
	render_tree($this->grouptree);

?>

	
</div>